defaults: &defaults
  base_url: http://example.com
  github:
    client_id: 'YOUR GITHUB CLINT ID'
    secret: 'YOUR GITHUB SECRET'
    login: 'YOUR GITHUB LOGIN' # optional
    token: 'YOUR GITHUB OAUTH TOKEN' # optional
  redis:
    url: redis://localhost:6379

development:
  <<: *defaults

test:
  <<: *defaults

production:
  <<: *defaults
